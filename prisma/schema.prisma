// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Products {
  id Int @id @default(autoincrement())
  name String @unique
  brand String
  category String @db.VarChar(155)
  price Int
  description String
  stock Int
  colors String
  sizes String
  images String
  discount Int
}